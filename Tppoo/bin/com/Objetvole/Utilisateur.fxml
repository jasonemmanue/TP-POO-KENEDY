<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="270.0" prefWidth="342.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controleur.UtilisateurController">
    <TextField fx:id="nomField" promptText="Nom" />
    <TextField fx:id="prenomField" promptText="Prénom" />
    <TextField fx:id="emailField" promptText="Email" />
    <TextField fx:id="telephoneField" promptText="Téléphone" />
    <Button onAction="#ajouterUtilisateur" text="Ajouter Utilisateur" />
</VBox>
